{
    "name": "Bookmarks in New Tab Page",
    "version": "1.0.0",
    "description": "Replaces the new tab page with tiled bookmarks",
    "permissions": [
        "bookmarks",
        "chrome://favicon/",
        "topSites",
        "tabs"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "icons": {
        "48": "small_tiles.png",
        "128": "small_tiles_128.png"
    },
    "manifest_version": 2
}
