{
    "name": "new tab bookmarks",
    "version": "0.1.0",
    "description": "an extension to add the tab with bookmarks",
    "permissions": [
        "bookmarks",
        "chrome://favicon/",
        "topSites",
        "tabs"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "icons": {
        "48": "small_tiles.png"
    },
    "manifest_version": 2
}
