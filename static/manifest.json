{
  "name": "Bookmarks in New Tab Page",
  "version": "1.7.0",
  "description": "Lightweight New Tab Page with your Bookmarks and Top Sites",
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "topSites",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "48": "small_tiles.png",
    "128": "small_tiles_128.png"
  },
  "manifest_version": 2
}
